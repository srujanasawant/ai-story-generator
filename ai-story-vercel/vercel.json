{
  "version": 2,
  "builds": [
    { "src": "api/generate.py", "use": "@vercel/python" },
    { "src": "static/**/*", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/generate", "dest": "api/generate.py" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
